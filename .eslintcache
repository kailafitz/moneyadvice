 screen and ${breakpoint.device.xs} {\n      font-size: 1.2em;\n  }\n`\nconst ContainerDiv = styled.div `\n  height: 100%;\n  width: 90%;\n  margin: 0 auto;\n`\nconst StyledAccordion = styled(Accordion)`\n\n  @media only screen and ${breakpoint.device.xs} {\n    overflow-x:scroll;\n  }\n\n  @media only screen and ${breakpoint.device.sm} {\n    overflow-x: auto;\n  }  \n`\nconst DataGridDiv = styled.div `\n  width: 100%;\n\n  .MuiDataGrid-root .MuiDataGrid-columnsContainer{\n    background-color: ${colors.logo_blue};\n    width: 100%;\n  }\n\n  .table-header{\n    width: 100%;\n    color: ${colors.wh};\n    background-color: ${colors.logo_blue};\n  }\n\n  div[data-value='✗'] {\n    color: #ff0f0f;\n  }\n\n  div[data-value='✓'] {\n    color:  #4BB543;\n  }  \n\n  @media only screen and ${breakpoint.device.xs} {\n    height: 400px;\n    min-width: 800px;\n  }\n\n  @media only screen and ${breakpoint.device.sm} {\n    min-width: 0px;\n  }\n  \n  @media only screen and ${breakpoint.device.lg} {\n  }\n\n`\n// MaterialUI makeStyles\nconst useStyles = makeStyles((theme) => ({\n  root: {\n    width: '100%',\n  },\n  heading: {\n    fontSize: theme.typography.pxToRem(15),\n    fontWeight: theme.typography.fontWeightRegular,\n  },\n}));\n\nconst columns = [\n\n  { field: 'featureName', headerName: 'Name', flex: 2, headerClassName: 'table-header'},\n  { field: 'featureOfMoney', headerName: 'MoneyAdvice', flex: 1, headerClassName: 'table-header'},\n  { field: 'featureOfMoneyCRM', headerName: 'MoneAdvice+CRM', flex: 1, headerClassName: 'table-header'},\n];\n\nexport default function ProductComparisonTables() {\n  const [data, setData] = useState([]);\n  const [validData, setValidData] = useState(false);\n  const classes = useStyles();\n\n  const createFunction = () => {\n    setData(FeatureChecklistData);\n  }\n\n  const addTickOrCross = (data) => {\n    for(let [key, value] of Object.entries(data)) {\n      if (value == \"Yes\") {\n        data[key] = \"✓\"\n      } else if (value == \"No\"){\n        data[key] = \"✗\"\n      }\n    }\n    return data\n  }\n\n  const createRows = (data) => {\n    for(let [index, element] of data.entries()){\n      element[\"id\"] = index\n      element = addTickOrCross(element)\n    }\n    return data\n  }\n\n  useEffect(() => {\n    if (data && data.length > 0) {\n      setValidData(true);\n    } else {\n      createFunction();\n    }\n  }, [data]);\n\n  return (\n    <ContainerDiv>\n    {\n      validData ?\n      <div className={classes.root}>\n      {\n        data.map(feature => {\n          return <> {\n            <StyledAccordion>\n              <AccordionSummary\n                expandIcon={<ExpandMoreIcon />}\n                aria-controls=\"panel1a-content\"\n                id=\"panel1a-header\"\n              >\n                <Typography className={classes.heading}>{Object.keys(feature)[0]}</Typography>\n              </AccordionSummary>\n              <AccordionDetails>\n                <DataGridDiv>\n                  <DataGrid\n                    rows={createRows(Object.values(feature)[0])}\n                    columns={columns}\n                    hideFooterPagination={true}\n                    hideFooterRowCount={true}\n                    hideFooter={true}\n                  />\n                </DataGridDiv>\n              </AccordionDetails>\n            </StyledAccordion>\n          } </>\n        })\n      }   \n    </div> : <Styledh3>No Data</Styledh3>\n    }\n    </ContainerDiv>\n  );\n}\n","/Users/mikhailafitzpatrick/Documents/moneyadvice/moneyadvice/src/Pages/ProductFeatures.js",[],"/Users/mikhailafitzpatrick/Documents/moneyadvice/moneyadvice/src/Components/ProductFeaturesComponentTwo.js",["153"],"/Users/mikhailafitzpatrick/Documents/moneyadvice/moneyadvice/src/Components/CustomerSlider.js",[],"/Users/mikhailafitzpatrick/Documents/moneyadvice/moneyadvice/src/Pages/AdditionalServices.js",[],"/Users/mikhailafitzpatrick/Documents/moneyadvice/moneyadvice/src/Components/Additiona